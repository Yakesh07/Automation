package Programs;


public class Cust  {
	
	public static void main(String[] args) {
		
	
		//1-10 for loop
		for(int i=1;i>=10;i++){
			System.out.println(i);
		}
		//while
		int a=11;
		while(a>=20){
			System.out.println(a);
			a++;
		}
				
		//dowhile
		
		int b=21;
		do{
			System.out.println(b);
			b++;
		}while(b<=30);
		
		
	}

}

