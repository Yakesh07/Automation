package Programs;

public class Hello {

		public static void main(String[] args) {
			
			int age=17;
			int hp=80;
			
			if(age>=18){
				System.out.println("Age is satisfied");
				if(hp>=50){
					System.out.println("Can donate the blood");
				}
				else{
					System.out.println("Cant donate the Blood");
				}
			}
			else{
				System.out.println("Age is Must Greater than 18");
			}
			
		}
		
		
		
		
		
		
		
		

		
}
